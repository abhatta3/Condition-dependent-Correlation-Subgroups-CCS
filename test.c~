#include<stdio.h>
#include <stdlib.h>

char ** data(char **vect,int D)
{
int i,j;

for (i=0;i<3;i++)  {
	for (j=0;j<D;j++)
        {
             printf("%c\t",vect[i][j]);
             vect[i][j]='3';
        }
            
        printf("\n");
} 
return vect; 

} 
void main()
{
char **vect;
int i,j,D=10;

vect=(char **)calloc(3,sizeof(char *));

for (i = 0; i < 3; i++)
	vect[i] = (char *)calloc(D,sizeof(char));
for (i=0;i<D;i++)
       vect[0][i]='1';

vect=data(vect, D);
for (i=0;i<3;i++)  {
	for (j=0;j<D;j++)
        {
             printf("%c\t",vect[i][j]);
             vect[i][j]='3';
        }
            
        printf("\n");
}


}
